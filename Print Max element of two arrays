#include<bits/stdc++.h>
using namespace std;

int main()
{
    int n;
    cin>>n;
    int t;
    cin>>t;
    
    int arr[t];
    for(int i=0;i<t;i++)
    {
        cin>>arr[i];
    }
    int max=INT_MIN;
    for(int i=0;i<t;i++)
    {
        if(max<arr[i])
        {
            max=arr[i];
        }
    }
    cout<<max<<endl;
    int k;
    cin>>k;
    int arr1[k];
    for(int i=0;i<k;i++)
    {
        cin>>arr1[k];
    }
    
    int mx=INT_MIN;
    for(int i=0;i<k;i++)
    {
        if(mx<arr1[i])
        {
            mx=arr1[i];
        }
        
    }
    cout<<mx<<endl;
    return 0;
    
}
